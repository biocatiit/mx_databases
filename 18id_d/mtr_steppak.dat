xenocsv_size   device motor slit_motor "" "" 0 0 -1e+09 1e+09 0 -1 -1 1 0 mm 0x1 2 xenocs_bot xenocs_top
xenocsh_center device motor slit_motor "" "" 0 0 -1e+09 1e+09 0 -1 -1 1 0 mm 0x1 1 xenocs_out xenocs_in
